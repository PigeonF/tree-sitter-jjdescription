{
  "name": "jjdescription",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          }
        ]
      }
    },
    "change": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "A"
              },
              {
                "type": "STRING",
                "value": "M"
              },
              {
                "type": "STRING",
                "value": "D"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "filepath"
        }
      ]
    },
    "filepath": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+?"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "JJ:"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_text_comment"
                },
                {
                  "type": "SYMBOL",
                  "name": "_change_comment"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_change_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[\\f\\v ]+"
        },
        {
          "type": "SYMBOL",
          "name": "change"
        }
      ]
    },
    "_text_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[\\f\\v ]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "[^AMD ]"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\\r\\n]+?"
          },
          "named": true,
          "value": "comment_text"
        }
      ]
    },
    "text": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^J\\n]"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n]+?"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\r"
    },
    {
      "type": "STRING",
      "value": "\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
